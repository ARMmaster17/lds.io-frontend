{ "endpoints": {
    "authorization": [{
      "method": "GET"
    , "parameters": "query"
    , "url": "https://lds.io/api/oauth3/authorize"
    }]
  , "access_token": [{
      "method": "POST"
    , "parameters": "urlencoded"
    , "url": "https://lds.io/api/oauth3/token"
    }]
  , "accounts": [{
      "url": "https://lds.io/api/oauth3/accounts"
    }]
  , "profile": [{
      "url": "https://lds.io/api/oauth3/accounts/{{account_id}}/me"
    }]
  , "inspect": [{
      "url": "https://lds.io/api/oauth3/inspect"
    }]
  , "logout": [{
      "url": "https://lds.io/api/oauth3/logout"
    }]
  , "switch_user": [{
      "url": "https://lds.io/api/oauth3/switch_user"
    }]
  , "scopes": [{
      "url": "https://lds.io/api/oauth3/scopes"
    }]
  }
, "strategies": [
    "authorization_code"
  , "implicit_grant"
  , "resource_owner_password"
  , "client_credentials"
  ]
}
